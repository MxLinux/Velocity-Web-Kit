{
    "name": "Velocity Web Kit",
    "version": "0.5",
    "description": "Modifications for Easton Velocity web-based tools",
    "permissions": [
        "activeTab",
        "http://10.20.31.144/potd"
    ],
    "content_scripts": [
        {
            "matches": ["http://eusvr41/internettechsupport/SearchTicket.aspx?TicketID=*"],
            "css": ["tickets.css"],
            "js": ["tickets.js"]
        },
        {
            "matches": ["http://eusvr41/internettechsupporttest/SearchTicket.aspx?TicketID=*"],
            "css": ["tickets.css"],
            "js": ["tickets.js"]
        },
        {
            "matches": ["https://noc.iglass.net/jglass/igo/*"],
            "js": ["iglass.js"]
        },
        {
            "matches": ["https://noc.iglass.net/*"],
            "css": ["iglass.css"]
        },
        {
            "matches": ["https://noc.iglass.net/jglass/igo/devAction*"],
            "css": ["iglass-events.css"]
        },
        {
            "matches": ["<all_urls>"],
            "js": ["gateway.js"]
        }
    ],
    "manifest_version": 2
}
