{
    "name": "Velocity Web Kit",
    "version": "0.5",
    "description": "Modifications for Easton Velocity web-based tools",
    "permissions": ["storage", "activeTab", "file:///home/shea/potd.txt"],
    "content_scripts": [
        {
            "matches": ["http://eusvr41/internettechsupport/SearchTicket.aspx?TicketID=*"],
            "css": ["tickets.css"],
            "js": ["tickets.js"]
        },
        {
            "matches": ["https://noc.iglass.net/*modem*"],
            "js": ["iglass.js"]
        },
        {
            "matches": ["https://noc.iglass.net/*"],
            "css": ["iglass.css"]
        },
        {
            "matches": ["<all_urls>"],
            "js": ["gateway.js"]
        },
        {
            "matches": ["file:///*"],
            "js": ["potd.js"]
        }
    ],
    "web_accessible_resources": [
        "//Euserver_5/isp/Arris Files/*.txt",
        "file://///Euserver_5/isp/Arris_Files/*.txt",
        "file:///home/shea/potd.txt",
        "/home/shea/potd.txt"
    ],
    "manifest_version": 2
}